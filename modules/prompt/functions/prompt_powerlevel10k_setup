# vim: ts=2 ft=zsh

zpromptpath="${ZDOTDIR:-$HOME/.zsh}"

[[ ! -f "${zpromptpath}/modules/prompt/external/p10k.zsh" ]] || \
	source "${zpromptpath}/modules/prompt/external/p10k.zsh"

[[ ! -f "${zpromptpath}/powerlevel10k/powerlevel10k.zsh-theme" ]] || \
	source "${zpromptpath}/powerlevel10k/powerlevel10k.zsh-theme"

unset zpromptpath
